@model Domain.Models.ServiceHistory

@{
    ViewData["Title"] = "Service History Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<div class="form-horizontal">
    <div class="col-md-10">
        <dl class="dl-horizontal">
            <dt>
                Serviceman
            </dt>
            <dd>
                @Html.DisplayFor(model => model.ServicemanFullName)
            </dd>
            <dt>
                Position
            </dt>
            <dd>
                @Html.DisplayFor(model => model.PositionTitle)
            </dd>
            <dt>
                Subdivision
            </dt>
            <dd>
                @Html.DisplayFor(model => model.SubdivisionTitle)
            </dd>
            <dt>
                Start Date
            </dt>
            <dd>
                @Html.DisplayFor(model => model.StartDate)
            </dd>
            <dt>
                End Date
            </dt>
            <dd>
                @Html.DisplayFor(model => model.EndDate)
            </dd>
        </dl>
        <div class="form-group">
            <div class="col-md-offset-2">
                @Html.ActionLink("Edit", "Edit", new { id = Model.Id }, new { @class = "btn btn-primary" })
                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
            </div>
        </div>
    </div>
</div>