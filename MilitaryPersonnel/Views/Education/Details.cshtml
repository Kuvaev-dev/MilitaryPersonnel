@model Domain.Models.Education

@{
    ViewData["Title"] = "Education Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<div class="form-horizontal">
    <div class="col-md-10">
        <dl class="dl-horizontal">
            <dt>
                Education Level
            </dt>
            <dd>
                @Html.DisplayFor(model => model.EducationLevel)
            </dd>
            <dt>
                Institution
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Institution)
            </dd>
            <dt>
                Specialty
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Specialty)
            </dd>
            <dt>
                Graduation Year
            </dt>
            <dd>
                @Html.DisplayFor(model => model.GraduationYear)
            </dd>
        </dl>
        <div class="form-group">
            <div class="col-md-offset-2">
                @Html.ActionLink("Edit", "Edit", new { id = Model.Id }, new { @class = "btn btn-primary" })
                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
            </div>
        </div>
    </div>
</div>